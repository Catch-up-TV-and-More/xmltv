<link rel="stylesheet" type="text/css" href="https://cdn.tvguide.co.uk/_ui/css/subscribeform2.css">
<script>
jQuery(function($) {
	
	$("a.topopup").click(function() {
			loading(); // loading
			setTimeout(function(){ 
				loadPopup(); 
			}, 500); // 
	return false;
	});
	
	$("div.close").hover(
					function() {
						$('span.ecs_tooltip').show();
					},
					function () {
    					$('span.ecs_tooltip').hide();
  					}
				);
	
	$("div.close").click(function() {
		disablePopup();  // function close pop up
	});
	
	$(this).keyup(function(event) {
		if (event.which == 27) { // 27 is 'Ecs' in the keyboard
			disablePopup();  // function close pop up
		}  	
	});
	
	$("div#backgroundPopup").click(function() {
		var err = document.getElementById("errEmail");
		if (err) {  err.style.display = 'none'; }
		disablePopup();  // function close pop up
	});
	$("img#NotNow").click(function() {
		var err = document.getElementById("errEmail");
		if (err) {  err.style.display = 'none'; }
		disablePopup();  // function close pop up
	});
		
	$('img#livebox').click(function() {
		
		var err = document.getElementById("errEmail");
		if (err) {  err.style.display = 'none'; }
		with (document.frmSub) {
			if (! validateEmail(subemail.value)) {
				if (err) {  err.style.display = 'block'; }
				return false;
			}
			submit();
		}
		
	});
	function validateEmail(email) { 
		var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		return re.test(email);
	} 

	 /************** start: functions. **************/
	function loading() {
		$("div.loader").show();  
	}
	function closeloading() {
		$("div.loader").fadeOut('normal');  
	}
	
	var popupStatus = 0; // set value
	
	function loadPopup() { 
		if(popupStatus == 0) { // if value is 0, show popup
			closeloading(); // fadeout loading
			$("#toPopup").fadeIn(0500); // fadein popup div
			$("#backgroundPopup").css("opacity", "0.7"); // css opacity, supports IE7, IE8
			$("#backgroundPopup").fadeIn(0001); 
			popupStatus = 1; // and set value to 1
		}	
	}
		
	function disablePopup() {
		if(popupStatus == 1) { // if value is 1, close popup
			setCookie();
			$("#toPopup").fadeOut("normal");  
			$("#backgroundPopup").fadeOut("normal");  
			popupStatus = 0;  // and set value to 0
		}
	}
	
	function setCookie()
	{
		var d = new Date();
		d.setTime(d.getTime()+(10*365*24*60*60*1000));
		var expires = "expires="+d.toGMTString();
		document.cookie = "showSub=N;path=/;" + expires;
	}
	/************** end: functions. **************/
}); // jQuery End
</script>

    
